                        <style>
                            .mapouter {
                                display: flex;
                                justify-content: center;
                                position: relative;
                                height: 400px;
                                width: 100%;
                                background: #fff;
                            }

                            .maprouter a {
                                color: #fff !important;
                                position: absolute !important;
                                top: 0 !important;
                                z-index: 0 !important;
                            }
                        </style>
                        
                    </div>
                </div>
            </div>

            <div class="container">
                <div class="row mt-5">

                    <div class="col-lg-4">
                        <div class="info">
                            <div class="address">
                                <i class="bi bi-geo-alt"></i>
                                <h4>Địa chỉ</h4>
                                <p>33 Xô Viết - Nghệ Tĩnh, Đà Nẵng</p>
                            </div>

                            <div class="email">
                                <i class="bi bi-envelope"></i>
                                <h4>Email:</h4>
                                <p>info@example.com</p>
                            </div>

                            <div class="phone">
                                <i class="bi bi-phone"></i>
                                <h4>Điện thoại:</h4>
                                <p>+1 5589 55488 55s</p>
                            </div>

                        </div>

                    </div>

                    <div class="col-lg-8 mt-5 mt-lg-0">
                        <div class="container">

                            <div class="section-title">
                                <h2>Đánh giá</h2>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-4 form-group">
                                <input type="text" name="name" id="name-feedback" class="form-control"
                                    placeholder="Tên của bạn" required>
                            </div>
                            <div class="col-md-4 form-group mt-3 mt-md-0">
                                <input type="email" class="form-control" id="email-feedback" name="email"
                                    placeholder="Email của bạn" required>
                            </div>
                            <div class="col-md-4 form-group mt-3 mt-md-0">
                               <select name="contentment-feedback" id="contentment-feedback" class="form-select mb-3">
                                    <option value="Rất hài lòng">Rất hài lòng</option>
                                    <option value="Hài lòng">Hài lòng</option>
                                    <option value="Không hài lòng">Không hài lòng</option>
                               </select>
                            </div>
